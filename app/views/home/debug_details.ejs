<!DOCTYPE html>
<html>

<head>
    <%- include('../shared/head.ejs') %>
</head>

<body class="theme-light">
<div class="page">
    <%- include('../shared/header.ejs') %>

    <div class="page-wrapper">
        <div class="container-xl">
            <!-- Page title -->
            <div class="page-header d-print-none">
                <div class="d-flex justify-content-between align-items-center">
                    <div class="">
                        <!-- Page pre-title -->
                        <div class="page-pretitle">
                            Debug
                        </div>
                        <h2 class="page-title">
                            Debug Data
                        </h2>
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <h3 style="margin: 30px 0 15px 0;"> Date: <%= details.created_at %> </h3>
            <% if(ping_id) { %>
                <p style="margin: 30px 0 15px 0;">
                    Click Ping ID:
                    <a target="_blank"
                       href="/<%= log_type %>/ping/<%= ping_id %>"> <%= ping_id %> </a>
                </p>
            <% } %>
            <div class="debug-data">
                <json-viewer>
                    <%= details.log %>
                </json-viewer>
            </div>
        </div>
        <%- include('../shared/footer.ejs') %>
    </div>
</div>
<script src="https://unpkg.com/@alenaksu/json-viewer@0.2.0/build/index.js"></script>
</body>
