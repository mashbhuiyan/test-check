<!DOCTYPE html>
<html>
<head>
    <%- include('../shared/head.ejs') %>
</head>
<body style="background-color: #F6F5F5">
<div class="container mt-2">
    <div class="justify-content-center row pt-5 mt-5">
        <div class="col-md-8 col-lg-6 col-xl-7">
            <div class="overflow-hidden card">
                <div class="bg-white">
                    <div class="row">
                        <div class="col-12">
                            <div class="text-center p-4">
                                <img src="data:image/png;base64,">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="pt-0 card-body p-5">
                    <div class="p-2">
                        <form class="form-horizontal" action="/create_session" method="post">
                            <% if (error) { %>
                                <div class="error" style="color: red;">
                                    <p>
                                        <%= error %>
                                    </p>
                                </div>
                            <% } %>
                            <% if (two_factor) { %>
                                <input name="email" placeholder="Enter email" type="hidden" required
                                       class="form-control form-control" aria-invalid="false" id="admin_user_email" value="<%= two_factor.username %>">

                                <input name="password" placeholder="Enter Password" type="hidden" class="form-control" value="<%= two_factor.password %>"
                                       aria-invalid="false" id="admin_user_password">

                                <div class="mb-4">
                                    <label class="form-label font-weight-semibold form-label" for="admin_user_otp">OTP Code</label>
                                    <input name="otp_code" placeholder="Enter OTP Code" type="password" class="form-control" required
                                           aria-invalid="false" id="admin_user_otp">
                                </div>

                                    <div class="text-end">
                                        <button class="btn btn-primary px-4 py-2" type="submit">Verify</button>
                                    </div>
                            <% } else { %>
                                <div class="mb-3">
                                    <label class="form-label font-weight-semibold form-label" for="admin_user_email">Email</label>
                                    <input name="email" placeholder="Enter email" type="email" required
                                           class="form-control form-control" aria-invalid="false" id="admin_user_email">
                                </div>
                                <div class="mb-4">
                                    <label class="form-label font-weight-semibold form-label" for="admin_user_password">Password</label>
                                    <input name="password" placeholder="Enter Password" type="password" class="form-control" required
                                           aria-invalid="false" id="admin_user_password">
                                </div>

                                    <div class="text-end">
                                        <button class="btn btn-primary px-4 py-2" type="submit">Log In</button>
                                    </div>
                            <% } %>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</body>